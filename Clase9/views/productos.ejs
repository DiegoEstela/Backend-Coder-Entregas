<!DOCTYPE html>
<html lang="en">
    <%- include('templates/head') %>
<body >
<div class="d-flex  justify-content-center">
    <div class=" w-50 p-3 ">
    <script> console.log(data)</script>
    <table class="table table-dark  table-striped">
        <thead>

          
          <tr>
            <th scope="col">#</th>
            <th scope="col">Producto</th>
            <th scope="col">Precio</th>
            <th scope="col">Url</th>
          </tr>
        </thead>
        <tbody>
            <% data.forEach(function(pro) { %>
          <tr>
            <th scope="row"><%- pro.id %></th>
            <td><%= pro.title %></td>
            <td><%= pro.price %></td>
            <td><%= pro.thumbnail %></td>
          </tr>
          <% }) %>
        </tbody>

        
      </table>
      <img src="https://ibb.co/album/B2gV62" width="200" alt="">
      <a class="btn btn-primary m-2 " href="/" role="button">Volver al Inico</a>
    <a class="btn btn-primary m-2" href="/form" role="button">Ir al Formulario</a>
    </div>
    </div>
        
</body>
</html>